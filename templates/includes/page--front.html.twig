{# empty Twig template #}
<div id="page" class="layout-container">
    {% include '@material_khphotography/includes/header.html.twig' %}
    <header id="header" role="banner">
        <div id="header-content">
            <div class="margin-box big-title text-align-center">
                <div class="title">
                    <h1>Kyle Huynh Photography</h1>
                </div>
                <div class="slogan h3">Avid Photographer and Programmer</div>
                <div class="header-buttons mt-48">
                    <a class="btn btn-raised slogan-btn-1" target="_blank" href="#" ><span style="position: relative; z-index: 2;"><span class="services-icons mr-8" data-icon="drupal"></span>My Photos</span><span class="rippleWrap" style="position: absolute; z-index: 1; left: 0px; top: 0px; overflow: hidden; border-radius: 2px; opacity: 1; transition: none 0s ease 0s;"><span class="rippleAnimate" style="position: absolute; left: 0px; top: 0px; width: 0px; height: 0px; border-radius: 50%; transform: scale3d(0, 0, 1); transition: none 0s ease 0s;"></span></span></a>
                    <a class="btn btn-raised icon-left slogan-btn-2" data-icon="forward" href="https://blog.kylehuynh.com" ><span>Visit my blog</span><span class="rippleWrap" style="position: absolute; z-index: 1; left: 0px; top: 0px; overflow: hidden; border-radius: 2px; opacity: 1; transition: none 0s ease 0s;"><span class="rippleAnimate" style="position: absolute; left: 0px; top: 0px; width: 0px; height: 0px; border-radius: 50%; transform: scale3d(0, 0, 1); transition: none 0s ease 0s;"></span></span></a>
                </div>
            </div>
        </div>
        <div id="header-bg">
            <div class="block">
                {# page.banner_photo #}
                {# page.banner_video #}
                {# page.banner_iframe #}
                <img id="bg-object" src="/sites/photo/files/banner.jpg">
            </div>
        </div>
    </header>

    {% if page.actions %}
        <div id="actions">
            {{ page.actions }}
        </div>
    {% endif %}
    <div id="content">
        {% if page.before_main %}
            <div id="before-main">
                {{ page.before_main }}
            </div>
        {% endif %}

        <main role="main" id="main" class="container">
            <div id="main-content" tabindex="-1">{# link is in html.html.twig #}

                <div class="card-holder">
                    {{ page.breadcrumb }}
                    {{ page.highlighted }}
                    {{ page.help }}

                    <div class="layout-content">
                        <div class="row card-grid">

                                {% for tid,photoset in photosets %}
                                    <div class="col-xs-12 col-sm-6 col-md-3 card-wrapper">
                                        <div class="card">
                                            <div class="card-item card-media">
                                                <a href="/node/{{ photoset.tid|e }}"><img src="{{ photoset.feature_img }}"></a>
                                                <h3 class="title pull-bottom"><a href="/node/{{ photoset.tid|e }}">{{ photoset.name }}</a></h3>
                                            </div>
                                        </div>
                                    </div>
                                {% endfor %}
                        </div>
                    </div>{# /.layout-content #}

                </div>{# /.card-holder #}

                {% if page.content_secondary %}
                    <div id="content-secondary">
                        {{ page.content_secondary }}
                    </div>
                {% endif %}
            </div>{# /#main-content #}

            {% include '@material_khphotography/includes/sidebar_left.html.twig' %}
            {% include '@material_khphotography/includes/sidebar_right.html.twig' %}


        </main>{# /#main #}

        {% if page.after_main %}
            <div id="after-main">
                {{ page.after_main }}
            </div>
        {% endif %}

    </div>{# /.content #}



    {% include '@material_khphotography/includes/footer.html.twig' %}

</div>