{# empty Twig template #}
<div id="page" class="layout-container">
    {% include '@material_khphotography/includes/header.html.twig' %}
    <header id="header" role="banner">
        <div id="header-content">
            <div class="margin-box big-title text-align-center">
                <div class="title">
                    <h1>{{ site_title }}</h1>
                </div>
                <div class="slogan h3">{{ slogan }}</div>

            </div>
        </div>
        <div id="header-bg">
            <div class="block">
                {# page.banner_photo #}
                {# page.banner_video #}
                {# page.banner_iframe #}
                <img id="bg-object" src="/sites/photo/files/banner.jpg">
            </div>
        </div>
    </header>

    {% if page.actions %}
        <div id="actions">
            {{ page.actions }}
        </div>
    {% endif %}
    <div id="content">
        {% if page.before_main %}
            <div id="before-main">
                {{ page.before_main }}
            </div>
        {% endif %}

        <main role="main" id="main" class="container">
            <div id="main-content" tabindex="-1">{# link is in html.html.twig #}

                <div class="card-holder">
                    {{ page.breadcrumb }}
                    {{ page.highlighted }}
                    {{ page.help }}

                    <div class="layout-content">
                        {{ page.content }}
                    </div>{# /.layout-content #}



                </div>{# /.card-holder #}

                {% if page.content_secondary %}
                    <div id="content-secondary">
                        {{ page.content_secondary }}
                    </div>
                {% endif %}
            </div>{# /#main-content #}

            {% include '@material_khphotography/includes/sidebar_left.html.twig' %}
            {% include '@material_khphotography/includes/sidebar_right.html.twig' %}


        </main>{# /#main #}

        {% if page.after_main %}
            <div id="after-main">
                {{ page.after_main }}
            </div>
        {% endif %}

        

    </div>{# /.content #}



    {% include '@material_khphotography/includes/footer.html.twig' %}

</div>